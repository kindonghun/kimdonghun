package day06;

import java.io.IOException;
import java.util.*;

public class test4 {

	public static boolean add(int n1) {

		
		if (n1 % 2 == 0) {
			return false;
		} else {
			return true;
		}
	}

	public static void aaa(int a, int b) {

		int sum = 0, i = 0;

		for (i = a; i <= b; i++)
			if (i % 2 == 1)
				sum += i;
		return ;
	}

	public static boolean bbb(char n1) {
		

		char data = n1;

		if (((int)data >= 65 && (int)data <= 90)) {
			return true;
		} else {
			return false;
		}
	}

	public static void main(String[] args) {
		System.out.println("Q.대문자소문자구별");
		if(bbb('B')) {
			System.out.println("대문자입니다.");
		}else
			System.out.println("소문자입니다.");
		
		System.out.println("Q.숫자를더해서 홀수값만출력");
		System.out.println(aaa(1.10));
		
		System.out.println("Q.홀수짝수구별");
		if(add(5)) {
			System.out.println("홀수입니다");
		}else
			System.out.println("짝수입니다");

		
	}

	private static char[] aaa(double d) {
		// TODO Auto-generated method stub
		return null;
	}

}
