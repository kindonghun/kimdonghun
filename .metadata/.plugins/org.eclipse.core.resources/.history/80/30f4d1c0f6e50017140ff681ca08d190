package day07;
import java.util.Scanner;


public class test1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
	
		int[] st=new int[4];
		st[0]=50;
		st[1]=50;
		st[2]=50;
		st[3]=(st[0]+st[1]+st[2]);
		st[4]=st[3]/3;
		
	
		System.out.println("국어점수:"+st[0]);
	
		System.out.println("수학점수:"+st[1]);
	
		System.out.println("영어점수:"+st[2]);
		
		System.out.println("합계"+st[3]);
		System.out.println("");
	
		
		
	}
}
