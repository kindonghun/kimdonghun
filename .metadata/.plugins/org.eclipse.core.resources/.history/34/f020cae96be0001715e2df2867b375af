package day04;

import java.util.Scanner;

public class q1 {
	public static void main(String[] args) {

		Scanner sc= new Scanner(System.in);
		System.out.println("말을탄시간을입력하세요");
		int a = sc.nextInt();
		int b = 10*500;
		//1분당 60*30
		
		System.out.println("계산:"+a*100+"원");
		System.out.println("추가요금:"+b);
		
		
			
	}
}
