package day05;

public class test3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int sum1=0;
		int sum2=0;
		
		for(int i=1;i<=100;i++) {
			 sum1 +=i;
			if(i%2==0) {
			sum2 += i;
			}
		}
		System.out.println("sum="+sum1);
		System.out.println("sum2="+sum2);
	}

}
