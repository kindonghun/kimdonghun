package day07;

public class test1 {
public static void main(String[] args) {
	int[] arr;
	arr = new int[5];
	for(int i=0;i<arr.length; i++) {
		arr[i]=i+1;
		
	}
	for(int i=0; i<5; i++) {
		System.out.println("arr["+i+"]:"+arr[i]);
	}
	
	
	int[] arr1= {1,2,3,4,5};
	for(int a:arr) {
		System.out.println(a);
	}
}
}
