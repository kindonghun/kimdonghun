package day05;

public class test2 {
	public static void main(String[] args) {
		int sum =0;
		for(int i=0;i<100;i++) {
			sum += i+1;
		}
		System.out.println("sum="+sum); 
		
		int a = 0;
		int b = 0;
		
		for(int c=0; c<100; c+=2) {
			a +=c+2;
			for(int d=0; d<100; d+=1) {
				b +=d+1;
			}
		}
		System.out.println(a);
		System.out.println(b);
	}
}
